<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,core.wcm.components.page.v2.editor,cq.wcm.msm.properties,granite.contexthub.configuration]"
    helpPath="https://www.adobe.com/go/aem_cmp_page_v2"
    mode="edit">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <advanced
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section1
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Settings"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <language
                                                granite:class="language"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                emptyText="Select"
                                                fieldLabel="Language"
                                                name="./jcr:language"
                                                renderReadOnly="{Boolean}true"
                                                translateOptions="{Boolean}true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="jcr:language"/>
                                                <datasource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/common/datasources/languages"
                                                    addNone="{Boolean}true"/>
                                            </language>
                                            <isLanguageRoot
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Must be checked if page is the root of a language copy."
                                                name="./cq:isLanguageRoot"
                                                renderReadOnly="{Boolean}true"
                                                text="Language Root"
                                                value="true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:isLanguageRoot"/>
                                            </isLanguageRoot>
                                            <redirect
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Redirect"
                                                name="./cq:redirectTarget"
                                                rootPath="/content">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:redirectPath"/>
                                            </redirect>
                                            <targetUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Open Redirct URL In"
                                                name="./cq:redirectTargetOption">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <newTab
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="New Tab"
                                                        value="_blank"/>
                                                    <sameWindow
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Same Window"
                                                        value="_self"/>
                                                    <newWindow
                                                        jcr:primaryType="nt:unstructured"
                                                        text="New Window"
                                                        value="newWindow"/>
                                                    <interstitial
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Interstitial Leaving The Site"
                                                        value="interstitialLeaving"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:redirectTargetOption"/>
                                            </targetUrl>
                                            <designs
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="wcm/designer/gui/components/designfield"
                                                fieldLabel="Design"
                                                name="./cq:designPath"
                                                renderReadOnly="{Boolean}true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:designPath"/>
                                            </designs>
                                            <alias
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Alias"
                                                name="./sling:alias"
                                                renderReadOnly="{Boolean}true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="sling:alias"/>
                                            </alias>
                                        </items>
                                    </section1>
                                    <configuration
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Configuration"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <cqConf
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/cloudconfig/components/admin/configpathbrowser"
                                                emptyText="Select Configuration Path"
                                                fieldLabel="Cloud Configuration">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:conf"/>
                                            </cqConf>
                                        </items>
                                    </configuration>
                                    <templates
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Templates Settings"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <allowedtemplates
                                                granite:class="foundation-layout-util-maximized-alt"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldDescription="Click 'Add Field' to add a template or a list of templates to be allowed as child pages. Each value in the list must be an absolute path to a template or use '/.*' to allow all templates below this path."
                                                fieldLabel="Allowed Templates"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    name="./cq:allowedTemplates"
                                                    renderReadOnly="{Boolean}true"/>
                                            </allowedtemplates>
                                        </items>
                                    </templates>
                                    <authenticationrequirement
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Authentication Requirement"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <cugconfigwarning
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/siteadmin/admin/properties/cugconfigwarning"
                                                warning="The Authentication Requirement cannot be edited because the page is using a deprecated configuration for the Closed User Group.&lt;br>Please refer to the &lt;a href=&quot;https://www.adobe.com/go/aem6_4_docs_cug_migration_en&quot; class=&quot;coral-Link&quot;>documentation&lt;/a>."/>
                                            <enable
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/siteadmin/admin/properties/authrequirement"
                                                fieldDescription="You might need to configure a Closed User Group in the Permission tab."
                                                text="Enable"/>
                                            <loginpath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/siteadmin/admin/properties/loginpath"
                                                fieldLabel="Login Page"
                                                name="./cq:loginPath"
                                                rootPath="/content">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:loginPath"/>
                                            </loginpath>
                                        </items>
                                    </authenticationrequirement>
                                    <export
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Export"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <exConfig
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Export Configuration"
                                                name="./cq:exportTemplate"
                                                optionLoader="cq.exportTemplate"
                                                renderReadOnly="{Boolean}true"
                                                rootPath="/etc/contentsync/templates">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:exportTemplate"/>
                                            </exConfig>
                                        </items>
                                    </export>
                                    <trainingConfig
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Training Configurations"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <isNotLinkable
                                             	jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/foundation/form/checkbox"
				                                fieldDescription="Must be checked if page needs to be set as Non Linkable"
				                                name="./cq:isNotLinkable"
				                                text="Is Not Linkable?"
				                                uncheckedValue="{Boolean}false"
				                                value="{Boolean}true"
				                                renderReadOnly="{Boolean}true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:isNotLinkable"/>
                                            </isNotLinkable>
                                            <isSpecialLink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Must be checked if the Page needs to be shown as a special link in Navigation"
                                                name="./cq:isSpecialLink"
                                                renderReadOnly="{Boolean}true"
                                                text="Special Link"
                                                value="true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-msm-lockable="cq:isSpecialLink"/>
                                            </isSpecialLink>
                                        </items>
                                    </trainingConfig>
                                </items>
                            </column>
                        </items>
                    </advanced>
                    <seo
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO and Analytics Configurations"
                        sling:orderBefore="cloudservices"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="SEO configurations"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <alwaysEnglishForSubBrand
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="This Field is for Analytics to track the Sub Brand Name in English"
                                                fieldLabel="Adobe Tracking Name"
                                                name="./adobeTrackingNameForSubBrand"/>
                                            <globalSeoTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure Global SEO Page Title for the page"
                                                fieldLabel="Global SEO Page Title"
                                                name="./globalSeoTitle"/>
                                            <seoTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure SEO Page Title for the page"
                                                fieldLabel="SEO Page Title"
                                                name="./seoTitle"/>
                                            <alwaysEnglish
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="This Field is for Analytics to track the Page Name in Always English"
                                                fieldLabel="Adobe Tracking Name"
                                                name="./adobeTrackingNameForPage"/>
                                            <metaDescription
                                            	cq:showOnCreate="{Boolean}false"
                                                cq:xfVariantType="web"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="configure meta Description"
                                                fieldLabel="Meta Description"
                                                height="20"
                                                name="./metaDescription"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <htmlRules
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="training/components/content/trainingRtePlugins/htmlRules"/>
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="training/components/content/trainingRtePlugins/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="training/components/content/trainingRtePlugins/uiSettings"/>
                                            </metaDescription>
                                            <canonicalUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Configure Canonical Url for the page"
                                                fieldLabel="Canonical Url"
                                                name="./canonicalUrl"/>
                                            <metaKeywords
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="SEO Meta Keywords">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                    name="./metaKeywords">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <Keyword
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Configure Meta Keyword for the page"
                                                                    fieldLabel="keyword"
                                                                    name="./keyword"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </metaKeywords>
                                            <robotsText
                                                cq:xfVariantType="web"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Robots Text"
                                                fieldLabel="Robots Text"
                                                height="20"
                                                name="./robotsText"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <htmlRules
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="training/components/content/trainingRtePlugins/htmlRules"/>
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="training/components/content/trainingRtePlugins/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="training/components/content/trainingRtePlugins/uiSettings"/>
                                            </robotsText>
                                            <metaImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldLabel="Meta Image"
                                                fileReferenceParameter="./metaImage"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,video/mp4,video/webm,video/ogg]"
                                                multiple="{Boolean}false"
                                                name="./metaNameImage"
                                                rootPath="/content/dam/"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                        </items>
                                    </section>
                                </items>
                            </column>
                        </items>
                    </seo>
                    <navigationConfig
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Navigation Image Config"
                        sling:orderBefore="cloudservices"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Page Background Configurations"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <navigationThumbnail
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Browse Thumbnail Image"
                                                fieldLabel="Navigation Thumbnail"
                                                fileReferenceParameter="./navThumbImg"
                                                name="./navThumbImage"
                                                rootPath="/content/dam"/>
                                            <navThumbAlt
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Alt text for Thumbnail"
                                                fieldLabel="Alt Text for Thumbnail Image"
                                                name="./navThumbAlt"
                                                required="{Boolean}false"/>
                                        </items>
                                    </section>
                                </items>
                            </column>
                        </items>
                    </navigationConfig>
                    <errorpage
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Error Page Configuration"
                        sling:orderBefore="cloudservices"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Error Page configurations"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <errorPage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Configure Error page for the site"
                                                fieldLabel="ErrorPage Url"
                                                name="./errorPages"/>
                                        </items>
                                    </section>
                                </items>
                            </column>
                        </items>
                    </errorpage>
                    <trainingconfig
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Training Configurations"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Client Library Configuration"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <clientlib
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="ClientLib Category Name"
                                                name="./clientlibCategory"/>
                                            <favIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Browse Logo/Image"
                                                fieldLabel="Fav Icon"
                                                fileReferenceParameter="./favIconSrcPolly"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
                                                multiple="{Boolean}false"
                                                name="./favIcon"
                                                rootPath="/content/dam"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                        </items>
                                    </section>
                                </items>
                            </column>
                        </items>
                    </trainingconfig>
                    <pageBackgroundConfig
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Page Background Config"
                        sling:orderBefore="cloudservices"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <section
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Page Background Configurations"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <backgroundColor
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                fieldLabel="Background Color:"
                                                name="./pageBgColor"/>
                                            <backgroundImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Background Image For Desktop"
                                                fieldLabel="Background Image For Desktop"
                                                name="./pageBgImg"
                                                rootPath="/content/dam"/>
                                            <backgroundImagemobile
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Background Image For Mobile"
                                                fieldLabel="Background Image For Mobile"
                                                name="./pageBgImgMble"
                                                rootPath="/content/dam"/>
                                            <backgroundoption
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Background Option"
                                                name="./backOption">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <image
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Image"
                                                        value="image"/>
                                                    <color
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Color"
                                                        value="color"/>
                                                </items>
                                            </backgroundoption>
                                        </items>
                                    </section>
                                </items>
                            </column>
                        </items>
                    </pageBackgroundConfig>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
